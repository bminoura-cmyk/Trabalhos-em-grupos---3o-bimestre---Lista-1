#include <stdio.h>

void fibonacci(int n, int *resultado) {
    if(n == 0) {
        *resultado = 1;
    } else if(n == 1) {
        *resultado = 1;
    } else {
        int a, b;
        fibonacci(n-1, &a);
        fibonacci(n-2, &b);
        *resultado = a + b;
    }
}

int main() {
    int N, FN;
    printf("Digite o número do mês: ");
    scanf("%d", &N);

    fibonacci(N, &FN);

    printf("Número de casais de coelhos no mês %d: %d\n", N, FN);
    return 0;
}
